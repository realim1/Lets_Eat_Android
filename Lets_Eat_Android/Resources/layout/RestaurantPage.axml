<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">
    <RelativeLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_margin="2.5dp"
        android:paddingBottom="4dp"
        android:scrollbars="vertical">
        <ImageView
            android:src="@android:drawable/ic_menu_gallery"
            android:id="@+id/photoImageView"
            android:layout_width="match_parent"
            android:layout_height="140dp"
            android:layout_gravity="center"
            android:scaleType="fitXY"
            android:padding="5dp" />
        <TextView
            android:id="@+id/nameTextView"
            android:layout_below="@id/photoImageView"
            android:text="Name"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingLeft="5dp" />
        <TextView
            android:id="@+id/cuisineTextView"
            android:layout_below="@id/nameTextView"
            android:text="Cuisine"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingLeft="5dp"
            android:layout_marginBottom="25.0dp" />
        <TextView
            android:id="@+id/reservation"
            android:layout_below="@id/cuisineTextView"
            android:text="Reservation"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingLeft="5dp"
            android:textColor="#ff000000" />
        <Button
            android:id="@+id/schedule_button"
            android:layout_below="@id/reservation"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Select a date"
            android:layout_marginBottom="25.0dp"
            android:background="@color/Lets_Eat_Orange" />
        <HorizontalScrollView
            android:id="@+id/horizontalScrollView1"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dip"
            android:layout_marginRight="5dip"
            android:layout_toLeftOf="@+id/btnNext"
            android:layout_toRightOf="@+id/btnPrevoius"
            android:layout_below="@id/reservation"
            android:scrollbars="none"
            android:fillViewport="true">
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_below="@id/reservation"
                android:layout_centerInParent="true"
                android:layout_margin="0dp"
                android:orientation="horizontal">
                <Button xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:layout_width="89dp"
                    android:layout_height="50dp"
                    android:id="@+id/timeButton1"
                    android:text="Time1"
                    android:layout_below="@id/reservation"
                    android:background="@color/Lets_Eat_Orange"
                    android:layout_marginRight="10.0dp" />
                <Button
                    android:layout_width="89dp"
                    android:layout_height="50dp"
                    android:id="@+id/timeButton2"
                    android:text="Time2"
                    android:layout_below="@id/reservation"
                    android:layout_toRightOf="@+id/timeButton1"
                    android:background="@color/Lets_Eat_Orange"
                    android:layout_marginRight="10.0dp" />
                <Button
                    android:layout_width="89dp"
                    android:layout_height="50dp"
                    android:id="@+id/timeButton3"
                    android:text="Time3"
                    android:layout_below="@id/reservation"
                    android:layout_toRightOf="@+id/timeButton2"
                    android:background="@color/Lets_Eat_Orange"
                    android:layout_marginRight="10.0dp" />
                <Button
                    android:layout_width="89dp"
                    android:layout_height="50dp"
                    android:id="@+id/timeButton4"
                    android:text="Time4"
                    android:layout_below="@id/reservation"
                    android:layout_toRightOf="@+id/timeButton3"
                    android:background="@color/Lets_Eat_Orange"
                    android:layout_marginRight="10.0dp" />
                <Button
                    android:layout_width="89dp"
                    android:layout_height="50dp"
                    android:id="@+id/timeButton5"
                    android:text="Time5"
                    android:layout_marginBottom="25.0dp"
                    android:layout_below="@id/timeButton1"
                    android:background="@color/Lets_Eat_Orange"
                    android:layout_marginRight="10.0dp" />
                <Button
                    android:layout_width="89dp"
                    android:layout_height="50dp"
                    android:id="@+id/timeButton6"
                    android:text="Time6"
                    android:layout_below="@id/timeButton2"
                    android:layout_marginBottom="25.0dp"
                    android:layout_toRightOf="@+id/timeButton5"
                    android:background="@color/Lets_Eat_Orange"
                    android:layout_marginRight="10.0dp" />
                <Button
                    android:layout_width="89dp"
                    android:layout_height="50dp"
                    android:id="@+id/timeButton7"
                    android:text="Time7"
                    android:layout_below="@id/timeButton3"
                    android:layout_marginBottom="25.0dp"
                    android:layout_toRightOf="@+id/timeButton6"
                    android:background="@color/Lets_Eat_Orange"
                    android:layout_marginRight="10.0dp" />
                <Button
                    android:layout_width="89dp"
                    android:layout_height="50dp"
                    android:id="@+id/timeButton8"
                    android:text="Time8"
                    android:layout_below="@id/timeButton4"
                    android:layout_marginBottom="25.0dp"
                    android:layout_toRightOf="@+id/timeButton7"
                    android:background="@color/Lets_Eat_Orange"
                    android:layout_marginRight="10.0dp" />
            </LinearLayout>
        </HorizontalScrollView>
        <TextView
            android:layout_marginTop="60dp"
            android:id="@+id/queue"
            android:layout_below="@id/reservation"
            android:text="Same Day Queue"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textColor="#ff000000"
            android:paddingLeft="5dp" />
        <Button
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:id="@+id/queueButton"
            android:text="Queue up"
            android:layout_below="@id/queue"
            android:background="@color/Lets_Eat_Orange" />
        <Button
            android:layout_below="@id/queueButton"
            android:layout_marginTop="20dp"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:id="@+id/menuButton"
            android:text="View Menu"
            android:layout_marginBottom="25.0dp"
            android:background="@color/Lets_Eat_Orange" />
        <TextView
            android:id="@+id/addressheader"
            android:layout_below="@id/menuButton"
            android:text="Address"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingLeft="5dp"
            android:textStyle="bold"
            android:layout_marginBottom="10.0dp"
            android:textColor="#ff000000" />
        <TextView
            android:id="@+id/addressdetail"
            android:layout_below="@id/addressheader"
            android:text="AddressDetail"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingLeft="15dp"
            android:layout_marginBottom="10.0dp"
            android:textColor="#ff000000" />
    <!--
        <TextView
            android:id="@+id/detail"
            android:layout_below="@id/addressdetail"
            android:text="Detail"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingLeft="5dp"
            android:layout_marginBottom="10.0dp"
            android:textColor="#ff000000" />
            -->
        <TextView
            android:id="@+id/phone"
            android:layout_below="@id/addressdetail"
            android:text="Phone"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textStyle="bold"
            android:paddingLeft="5dp"
            android:textColor="#ff000000" />
        <TextView
            android:id="@+id/phonedetail"
            android:layout_below="@id/phone"
            android:text="PhoneDetail"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginBottom="10.0dp"
            android:paddingLeft="15dp"
            android:textColor="#ff000000" />
        <TextView
            android:id="@+id/hours"
            android:layout_below="@id/phonedetail"
            android:text="Hours"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textStyle="bold"
            android:paddingLeft="5dp"
            android:textColor="#ff000000" />
        <TextView
            android:id="@+id/hoursdetail"
            android:layout_below="@id/hours"
            android:text="HoursDetail"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginBottom="10.0dp"
            android:paddingLeft="15dp"
            android:textColor="#ff000000" />
        <TextView
            android:id="@+id/description"
            android:layout_below="@id/hoursdetail"
            android:text="Description"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textStyle="bold"
            android:paddingLeft="5dp"
            android:textColor="#ff000000" />
        <TextView
            android:id="@+id/descriptiondetail"
            android:layout_below="@id/description"
            android:text="DescriptionDetail"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingLeft="15dp"
            android:textColor="#ff000000" />
    </RelativeLayout>
</ScrollView>